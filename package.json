{
    "name": "@clowder-generator/utils",
    "version": "2.0.1",
    "description": "A utils collection to help building clowder related generator",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "scripts": {
        "test": "jest --passWithNoTests --config jest.config.json",
        "coverage": "npm run test -- --coverage",
        "coverage:istanbul": "nyc npm run test",
        "mutation": "npx stryker run",
        "clean": "rm -rf dist .stryker-tmp",
        "prebuild": "npm run clean",
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "prepare": "husky install"
    },
    "exports": {
        ".": {
            "node": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./case-helper": {
            "node": "./dist/case-helper/index.js",
            "require": "./dist/case-helper/index.js"
        },
        "./context-helper": {
            "node": "./dist/context-helper/index.js",
            "require": "./dist/context-helper/index.js"
        },
        "./destination-path-processor-helper": {
            "node": "./dist/destination-path-processor-helper/index.js",
            "require": "./dist/destination-path-processor-helper/index.js"
        },
        "./path-helper": {
            "node": "./dist/path-helper/index.js",
            "require": "./dist/path-helper/index.js"
        },
        "./string-helper": {
            "node": "./dist/string-helper/index.js",
            "require": "./dist/string-helper/index.js"
        },
        "./validator-helper": {
            "node": "./dist/validator-helper/index.js",
            "require": "./dist/validator-helper/index.js"
        },
        "./package.json": "./package.json"
    },
    "keywords": [
        "clowder",
        "lib",
        "utils"
    ],
    "author": "leddzip",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "https://github.com/clowder-generator/clowder-utils"
    },
    "dependencies": {
        "@types/yeoman-generator": "^5.2.11",
        "typescript": "^4.9.4"
    },
    "devDependencies": {
        "@stryker-mutator/core": "^6.3.1",
        "@stryker-mutator/jest-runner": "^6.3.1",
        "@stryker-mutator/typescript-checker": "^6.3.1",
        "@types/jest": "^29.2.4",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "eslint": "^8.31.0",
        "eslint-config-standard-with-typescript": "^27.0.1",
        "eslint-plugin-import": "^2.27.4",
        "eslint-plugin-n": "^15.6.1",
        "eslint-plugin-promise": "^6.1.1",
        "husky": "^8.0.3",
        "jest": "^29.3.1",
        "memfs": "^3.4.12",
        "nyc": "^15.1.0",
        "ts-jest": "^29.0.3",
        "ts-mockito": "^2.6.1",
        "unionfs": "^4.4.0"
    }
}
